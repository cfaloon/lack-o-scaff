<h1>All Products</h1>

<table>
  <tr>
    <th>Name</th><th>Price</th><th>Quantity</th>
  </tr>

  <% @products.each do |product| %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.price %></td>
    <td><%= product.quantity%></td>
  </tr>
  <% end %>
</table>

<%= link_to 'Back', root_path %>
